<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.WhOperatorMapper">


    <!-- 查找详情 2019-12-10-->
    <select id="getById" resultType="com.apl.wms.wh.po.WhOperatorPo">
        SELECT
        id, 
        member_id, 
        member_name, 
        wh_id, 
        inner_org_id
        FROM
        wh_operator
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-10-->
    <select id="getList" resultType="com.apl.wms.wh.vo.WhOperatorListVo">
        SELECT
        id, 
        member_id, 
        member_name, 
        wh_id, 
        inner_org_id
        FROM
        wh_operator
        where
        1 = 1
    </select>

</mapper>
