<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.OutOrderCommodityMapper">


    <!-- 查找详情 2019-12-11-->
    <select id="getById" resultType="com.apl.wms.wh.vo.OutOrderCommodityInfoVo">
        SELECT
        id, 
        order_id, 
        commodity_id, 
        commodity_name, 
        commodity_qty, 
        inner_org_id
        FROM
        out_order_commodity
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-11-->
    <select id="getList" resultType="com.apl.wms.wh.vo.OutOrderCommodityListVo">
        SELECT
        id, 
        order_id, 
        commodity_id, 
        commodity_name, 
        commodity_qty, 
        inner_org_id
        FROM
        out_order_commodity
        where
        1 = 1
    </select>

</mapper>
