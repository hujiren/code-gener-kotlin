<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.WhZoneMapper">


    <!-- 查找详情 2019-12-11-->
    <select id="getById" resultType="com.apl.wms.wh.vo.WhZoneInfoVo">
        SELECT
        id, 
        wh_id, 
        zone_code, 
        zone_name, 
        zone_type, 
        inner_org_id
        FROM
        wh_zone
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-11-->
    <select id="getList" resultType="com.apl.wms.wh.vo.WhZoneListVo">
        SELECT
        id, 
        wh_id, 
        zone_code, 
        zone_name, 
        zone_type, 
        inner_org_id
        FROM
        wh_zone
        where
        1 = 1
    </select>

</mapper>
