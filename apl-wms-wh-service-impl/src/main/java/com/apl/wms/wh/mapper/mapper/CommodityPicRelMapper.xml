<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.CommodityPicRelMapper">


    <!-- 查找详情 2019-12-10-->
    <select id="getById" resultType="com.apl.wms.wh.po.CommodityPicRelPo">
        SELECT
        id, 
        community_id, 
        img_url, 
        img_sort, 
        inner_org_id
        FROM
        commodity_pic_rel
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-10-->
    <select id="getList" resultType="com.apl.wms.wh.vo.CommodityPicRelListVo">
        SELECT
        id, 
        community_id, 
        img_url, 
        img_sort, 
        inner_org_id
        FROM
        commodity_pic_rel
        where
        1 = 1
    </select>

</mapper>
