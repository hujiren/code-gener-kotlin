<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.WarehouseMapper">


    <!-- 查找详情 2019-12-11-->
    <select id="getById" resultType="com.apl.wms.wh.vo.WarehouseInfoVo">
        SELECT
        id, 
        wh_code, 
        wh_name, 
        wh_name_en, 
        wh_status, 
        wh_type, 
        local_time_zone, 
        inner_org_id
        FROM
        warehouse
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-11-->
    <select id="getList" resultType="com.apl.wms.wh.vo.WarehouseListVo">
        SELECT
        id, 
        wh_code, 
        wh_name, 
        wh_name_en, 
        wh_status, 
        wh_type, 
        local_time_zone, 
        inner_org_id
        FROM
        warehouse
        where
        1 = 1
    </select>

</mapper>
