<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.ReceivingItemMapper">


    <!-- 查找详情 2019-12-11-->
    <select id="getById" resultType="com.apl.wms.wh.vo.ReceivingItemInfoVo">
        SELECT
        id, 
        receiving_id, 
        sub_no, 
        sku, 
        length, 
        width, 
        height, 
        volume, 
        gw, 
        inner_org_id
        FROM
        receiving_item
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-11-->
    <select id="getList" resultType="com.apl.wms.wh.vo.ReceivingItemListVo">
        SELECT
        id, 
        receiving_id, 
        sub_no, 
        sku, 
        length, 
        width, 
        height, 
        volume, 
        gw, 
        inner_org_id
        FROM
        receiving_item
        where
        1 = 1
    </select>

</mapper>
