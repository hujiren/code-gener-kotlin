<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apl.wms.wh.mapper.CommodityBrandMapper">


    <!-- 查找详情 2019-12-11-->
    <select id="getById" resultType="com.apl.wms.wh.vo.CommodityBrandInfoVo">
        SELECT
        id, 
        brand_name, 
        brand_name_en, 
        cat_id, 
        cust_id, 
        inner_org_id
        FROM
        commodity_brand
        where
            id = #{id}
    </select>

    <!-- 查找列表 2019-12-11-->
    <select id="getList" resultType="com.apl.wms.wh.vo.CommodityBrandListVo">
        SELECT
        id, 
        brand_name, 
        brand_name_en, 
        cat_id, 
        cust_id, 
        inner_org_id
        FROM
        commodity_brand
        where
        1 = 1
    </select>

</mapper>
